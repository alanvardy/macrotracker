<p id="notice"><%= notice %></p>

<h1>Weighings</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Day</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weighings.each do |weighing| %>
      <tr>
        <td><%= weighing.weight %></td>
        <td><%= weighing.day %></td>
        <td><%= weighing.user %></td>
        <td><%= link_to 'Show', weighing %></td>
        <td><%= link_to 'Edit', edit_weighing_path(weighing) %></td>
        <td><%= link_to 'Destroy', weighing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weighing', new_weighing_path %>
